{
  "name": "winter-love-graphql-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "git@github.com:bichikim/winter-love-graphql-server.git",
  "author": "Bichi Kim <bichi@live.co.kr>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --transpileOnly -r tsconfig-paths/register --project tsconfig.node.json src/index.ts",
    "build": "tsc -p tsconfig.amd.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "start": "ts-node -r tsconfig-paths/register --project tsconfig.node.json src/index.ts",
    "doc": "typedoc -out ./docs ./src --media ./media",
    "test": "npm run test:unit",
    "lint": "eslint \"src/\"",
    "test:unit": "mocha \"test/**/*.spec.ts\"",
    "test:watch": "",
    "preversion": "npm run test && npm run build && npm run build:esm"
  },
  "dependencies": {
    "apollo-server": "^2.8.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "dotenv": "^8.1.0",
    "globby": "^10.0.1",
    "graphql": "^14.5.0",
    "graphql-redis-subscriptions": "^2.1.1",
    "graphql-subscriptions": "^1.1.0",
    "mongoose": "^5.6.10",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.5"
  },
  "devDependencies": {
    "@types/ioredis": "^4.0.15",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.13",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.2.0",
    "mongodb-memory-server": "^5.2.0",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.19.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  }
}
